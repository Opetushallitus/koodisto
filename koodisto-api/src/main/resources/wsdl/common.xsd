                name="sitovuustaso" type="xs:string" />
            <xs:element minOccurs="0" maxOccurs="1"
                name="toimintaymparisto" type="xs:string" />
            <xs:element minOccurs="0" maxOccurs="1"
                name="tarkentaaKoodistoa" type="xs:string" />
            <xs:element minOccurs="0" maxOccurs="1"
                name="huomioitavaKoodisto" type="xs:string" />
