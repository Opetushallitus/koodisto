            <xs:element name="fieldErrors" minOccurs="0"
                maxOccurs="unbounded" type="tns:FieldErrorType" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="FieldErrorType">
        <xs:sequence>
