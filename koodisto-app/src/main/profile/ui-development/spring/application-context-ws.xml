<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
    ">

    <bean id="mockDataHandler" class="fi.vm.sade.koodisto.service.mock.MockDataHandler" />
    <bean id="koodistoService" class="fi.vm.sade.koodisto.service.mock.KoodistoServiceMock">
        <property name="mockDataHandler" ref="mockDataHandler" />
    </bean>
    <bean id="koodiService" class="fi.vm.sade.koodisto.service.mock.KoodiServiceMock">
        <property name="mockDataHandler" ref="mockDataHandler" />
    </bean>
    <bean id="uploadService" class="fi.vm.sade.koodisto.service.mock.UploadServiceMock" />
    <bean id="downloadService" class="fi.vm.sade.koodisto.service.mock.DownloadServiceMock" />
    <bean id="organisaatioService" class="fi.vm.sade.organisaatio.api.OrganisaatioServiceMock" />

</beans>
